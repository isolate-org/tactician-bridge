{
  "name": "isolate/tactician-bridge",
  "description": "Provides integration for Tactician with Isolate",
  "keywords": ["isolate", "tactician", "command bus"],
  "require": {
    "php": ">=5.5.0",
    "league/tactician": "^0.6 || ^1.0",
    "isolate/isolate": "1.0.*@dev",
    "isolate/unit-of-work": "1.0.*@dev",
    "isolate/lazy-objects": "1.0.*@dev"
  },
  "require-dev": {
    "phpspec/phpspec": "2.1.*"
  },
  "autoload": {
    "psr-0": {
      "Isolate": "src/",
      "Isolate\\Tests": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Norbert Orzechowicz",
      "email": "norbert@orzechowicz.pl"
    }
  ]
}